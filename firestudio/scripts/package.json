{
  "name": "agricultural-policies-importer",
  "version": "1.0.0",
  "description": "Import agricultural policies from CSV to Supabase",
  "main": "importPolicies.ts",
  "scripts": {
    "import": "tsx importPolicies.ts",
    "import:dev": "tsx --watch importPolicies.ts",
    "import:schemes": "tsx importGovernmentSchemes.ts",
    "import:state-based": "tsx importStateBasedSchemes.ts",
    "test:connection": "tsx testConnection.ts",
    "schema:check": "echo 'Run the schema.sql file in your Supabase SQL Editor'"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "csv-parser": "^3.0.0",
    "papaparse": "^5.4.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/papaparse": "^5.3.14",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
